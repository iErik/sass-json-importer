{
  "name": "@blakedarlin/sass-json-importer",
  "version": "1.1.0",
  "description": "Allows importing JSON in Sass files parsed by sass/sass-embedded, with added support for WordPress.",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "license": "MIT",
  "repository": "github:blakedarlin/sass-json-importer",
  "keywords": [
    "sass",
    "sass-embedded",
    "importer",
    "json",
    "wordpress"
  ],
  "author": {
    "name": "Blake Darlin",
    "email": "hello@blakedarlin.com",
    "url": "https://blakedarlin.com"
  },
  "files": [
    "./dist/index.js",
    "./dist/importer.js",
    "./dist/*.d.ts"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --runInBand --watchAll --config ./jest.config.js",
    "coverage": "NODE_OPTIONS=--experimental-vm-modules npx jest --config ./jest.config.js --coverage",
    "build": "tsc -p tsconfig.build.json",
    "watch": "tsc -p tsconfig.build.json -watch",
    "format": "prettier --write \"**/*.{js,ts,json}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json}\"",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@jest/types": "^29.6.3",
    "@swc/core": "1.7.26",
    "@swc/jest": "^0.2.36",
    "@tsconfig/node20": "^20.1.4",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-unicorn": "^55.0.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.79.4",
    "ts-jest": "^29.2.5",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0"
  }
}
